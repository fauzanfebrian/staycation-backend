<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="nav-home-tab">
    <!-- DataTales Example -->
    <div class="row">
        <% for( let i = 0; i < item.imageId.length; i++ ) { %>
        <div class="col-4">
            <div class="card shadow my-3">
                <div class="card-body">
                    <img src="http://localhost:3000/<%= item.imageId[i].imageUrl %> " alt="img"
                        style="width: 100%; display: block;">
                </div>
            </div>
        </div>
        <% } %>
    </div>
    <a href="/admin/item" class="btn btn-outline-secondary">back</a>
    <!--End DataTables -->

</div>